    <xs:include schemaLocation="issueTrackers/issueTrackers.xsd"/>
    <xs:complexType name="settingsType">
        <xs:sequence>
            <xs:element type="settingType" name="setting" maxOccurs="unbounded" minOccurs="0"/>
